{
  "name": "thesis-server",
  "version": "0.0.0",
  "description": "Wow.",
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@types/jest": "~29.2",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.3",
    "prettier": "~2.8",
    "ts-jest": "~29.0",
    "tsc-alias": "^1.8.3",
    "tsx": "^3.12.5",
    "typescript": "~4.9"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node --experimental-specifier-resolution=node dist/server.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.production.json && tsc-alias -p tsconfig.production.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lightning": "^7.1.4",
    "socket.io": "^4.6.1"
  }
}
